{
    "description": "Easy web server monitoring.",
    "homepage": "https://jmonitor.io",
    "name": "johndodev/jmonitor-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["monitoring", "mysql", "apache", "metrics", "symfony"],
    "authors": [
        {
            "name": "Johndodev",
            "homepage": "https://jonathan-plantey.fr/"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "symfony/console": "^7.1",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/http-client": "^6.4|^7.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Johndodev\\JmonitorBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Symfony\\JmonitorBundle\\Tests\\": "tests/" }
    }
}
