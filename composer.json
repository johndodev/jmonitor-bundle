{
    "description": "Easy web server monitoring.",
    "homepage": "https://jmonitor.io",
    "name": "johndodev/jmonitor-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["monitoring", "mysql", "apache", "metrics", "symfony"],
    "authors": [
        {
            "name": "Johndodev",
            "homepage": "https://jonathan-plantey.fr/"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "php-http/discovery": "^1.20",
        "psr/http-client": "^1.0",
        "symfony/console": "^7.1",
        "symfony/framework-bundle": "^7.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": { "Johndodev\\JmonitorBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Symfony\\JmonitorBundle\\Tests\\": "tests/" }
    },
    "require-dev": {
        "psr/http-factory": "^1.1"
    }
}
