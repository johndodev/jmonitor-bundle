{
    "description": "Easy web server monitoring.",
    "homepage": "https://jmonitor.io",
    "name": "johndodev/jmonitor-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["monitoring", "mysql", "php", "redis", "apache", "nginx", "system", "metrics", "symfony"],
    "authors": [
        {
            "name": "Johndodev",
            "homepage": "https://jonathan-plantey.fr/"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^2.13",
        "php-http/discovery": "^1.20",
        "psr/http-client": "^1.0",
        "psr/log": "^1.1|^2.0|^3.0",
        "symfony/console": "^6.4|^7.0",
        "symfony/framework-bundle": "^6.4|^7.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        },
        "platform": {
            "php": "8.1"
        }
    },
    "autoload": {
        "psr-4": { "Johndodev\\JmonitorBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Symfony\\JmonitorBundle\\Tests\\": "tests/" }
    },
    "require-dev": {
        "psr/http-factory": "^1.1"
    }
}
